	.internal-alias :main_external,     -1
	.internal-alias :authenticate,      2

    .global-base        0
    .persistent-base    1000000

    .selector
    SETCP0
    PUSHINT $y_persistent$$
    PUSHROOT CTOS
    PUSHINT 64
    DICTIGET
    THROWIFNOT 103
    LDI 256 ENDS
    PUSHINT 127
    EQUAL
    THROWIFNOT 101
    CALL $:authenticate$

    PUSHINT $a$
    PUSH c5 CTOS
    PUSHINT 64
    DICTIGET
    THROWIFNOT 102
    LDI 256 ENDS
    PUSHINT 1 
    EQUAL
    THROWIFNOT 104

    .internal   :main_external
    PUSHINT 10

    .internal   :authenticate
    DUP

    .globl  x
    .type   x,  @function
x:
    RET         ;contract ignores internal msgs by default 

    .data
    .globl  y_persistent
    .align 16
    .type   y_persistent,  @object
    .size   y_persistent,  1
y_persistent:
    .byte   127

    .globl  z
z:
    CTOS
    .type   z,  @function

    .globl  a
    .align  16
    .type   a, @object
    .size   a, 16
a:
    .long   1
    .long   2
    .long   3
    .long   4
